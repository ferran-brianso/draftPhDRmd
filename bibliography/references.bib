	keywords = {animal models, Animals, bioinformatics, biomarker: diagnostic, biomarker: prognostic, biomarkers, Biomarkers, Brain, Catenins, Delta Catenin, Female, Gene Expression Profiling, Gene Expression Regulation, Humans, Intracellular Signaling Peptides and Proteins, Ischemic Stroke, Male, Mice, Inbred C57BL, Prognosis, Proteome, Proteomics, Stroke biology, systems biology, Transcriptome},
	pages = {1921--1936},
