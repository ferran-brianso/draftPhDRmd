	pmcid = {PMC4941301},
	pages = {16112--16129},
	file = {PubMed Central Full Text PDF:C\:\\Users\\briansof\\Zotero\\storage\\K4VWLWPF\\Rodríguez-Hernández et al. - 2016 - Cinacalcet inhibits neuroblastoma tumor growth and.pdf:application/pdf},
